<h1>新規投稿フォーム</h1>
  <%= form_with model: @post do |f| %>
    <h4>画像・写真</h4>
      <%= f.file_field :image, accept: "image/*" %><br/>

    <h4>投稿名</h4>
      <%= f.text_field :post_name %><br>

    <h4>説明</h4>
      <%= f.text_area :caption %>

    <h4>URL</h4>
    <%= f.text_field :URL%></br>


  <%= f.submit '投稿' %></br>

  <% end %>
